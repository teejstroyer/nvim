{
    "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
    "Lua.diagnostics.disable": [
        "unused-local",
        "undefined-global",
        "undefined-field",
        "param-type-mismatch",
        "missing-parameter",
        "missing-fields"
    ],
    "Lua.diagnostics.globals": [
        "vim",
        "packer_bootstrap",
        "use",
        "inoremap",
        "nnoremap",
        "tnoremap",
        "bufnr",
        "vnoremap",
        "MiniDeps"
    ]
}